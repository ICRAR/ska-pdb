.digg_pagination
	.page_info 
		= page_entries_info @parameters
	= will_paginate @parameters, :container => true
#search_results
	%table.zebra-striped
		%thead
			%th Unit
			%th Source
			%th Expression
			%th Description
		%tbody
			- @parameters.each do |p|
				%tr.parameter
					%td 
						= p.unit
					%td 
						= p.source
					%td 
						= p.expression
					%td 
						= p.description
